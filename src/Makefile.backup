#endif

ifeq ($(DEBUG_BUILD),1)
	DBG_OPT += -g
# supposed to be the same as -g but much faster to link statically
# - testing show otherwise
#	DBG_OPT += -gstabs+
	DBG_OPT += -Werror
	DBG_OPT += -fpic
#	DBG_OPT += -fPIC
	DBG_OPT += -D_FILE_OFFSET_BITS=64
#	DBG_OPT += -O0
	DBG_OPT += -O3
#	DBG_OPT += -march=pentium4
#	DBG_OPT += -fomit-frame-pointer
#	DBG_OPT += -fvisibility-inlines-hidden
	DBG_OPT += -fno-strict-aliasing
#	DBG_OPT += -DUSE_ELOOP_GLIB
	DBG_OPT += -DUSE_ELOOP_LEVT
#	DBG_OPT += -DUSE_HOST2IP_SYNC
	DBG_OPT += -DUSE_HOST2IP_FORK
#	DBG_OPT += -DUSE_HOST2IP_AGAI
	DBG_OPT += -DNO_DBG_TRACE=1
#	DBG_OPT += -DNO_DBG_ASSERT=1
	DBG_OPT += -DNO_TOKEEP_CHECK_DECLARE=1
	DBG_OPT += -DNO_TOKEEP_CHECK_USAGE=1
	DBG_OPT += -DNO_NIPMEM_SMASHING=1
	DBG_OPT += -DNO_NIPMEM_TRACKING=1
# jme- to make the template instance unique at link-time. i dont really get what it is or how to use it tho
#      would make the code smaller. i tried and it created compilation error at link time
#	DBG_OPT += -frepo
# jme- for stuff i dont really get about precompiled header (not important as they are not compatible with distcc)
#	DBG_OPT += -fpch-preprocess
else
	DBG_OPT += -O3 -DNEOIP_NODEBUG_COMPIL=1 -DNO_DBG_TRACE=1
#-DNO_TRACE_AT_ALL
endif

