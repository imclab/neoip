#######################################
# to create the .include directory
# - it link all the .hpp of the src/ directory in a single directory
# - this avoid having a lot of -I in the command line.
#   - i did the 'lots of -I' before and actually reached the 32kbyte limit
# - this directory is ONLY for the building and NOT anything else.
# - TODO to replace with ln -s `find ../src -name "*.hpp"` . which is MUCH faster
######
build_include_dir:
	cd ../.include && rm -f ../.include/*.hpp && for i in `find ../src -name "*.hpp"`; do ln -s $$i .; done

clean_include_dir:
	find ../.include -type l -name "*.hpp" | xargs rm -f

#######################################
# mirror the src directory structure in the build directory
# - this is required in order to later write the .o in them
# - so it is required to be uptodate and done before compilation
mirror_build_dir:
	find ../src/. -type d| sed 's/^\.\.\/src\///g' | xargs mkdir -p

# remove all the empty directory in the current directory
clean_build_dir:
	find ./* -type "d" | sort -r | xargs -r rmdir

#######################################
# clean all the temporary file in the config_dir_debug1 and 2 directory
######
clean_config_debug_dir:
	(cd ../src/config_dir_debug1 && rm -f tmp/*.log)
	(cd ../src/config_dir_debug1 && rm -rf tmp/neoip_*_pfile*)
	(cd ../src/config_dir_debug2 && rm -f tmp/*.log)
	(cd ../src/config_dir_debug2 && rm -rf tmp/neoip_*_pfile*)
	
#######################################
# clean all the temporary file in ../pkg directory
######
clean_pkg:
	make -C ../pkg clean

#######################################
# to make a fast dependancies computation (may be inaccurate in special cases tho :)
######
dep:
	(cd $(SRC_DIR) && ./mkfiles.pl && ./mkdep.pl)

# to include 
ifeq ($(SRC_DIR)/make.dep,$(wildcard $(SRC_DIR)/make.dep))
include $(SRC_DIR)/make.dep
endif

#######################################
# to make a slow dependancies computation (slow but surely accurate)
######
# only GCC. 
depend slowdep:
	@echo Start to build depedances in .depend
	for i in `find apps neoip*/ -name "*.c-removemetohaveCcode" -or -name "*.cpp"`; do echo Building dependencies for $$i >/dev/tty; echo -n `dirname $$i`/;$(CPP) -M $(CFLAGS) $$i;done >.tmpdepend
	mv -f .tmpdepend .depend
	@echo Dependances completed

#only GCC and good make
ifeq (.depend,$(wildcard .depend))
include .depend
endif



