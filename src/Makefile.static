# make target to strip all static libraries
# - usefull to make them smaller and especially link faster
strip_static_lib:
	$(STRIP) --strip-debug *.a

# some experiment to remove all nunit code from the static libraries
# - NOTE: done during an experiement to link faster via smaller .a on win32
remove_nunit_static_lib: strip_static_lib
	(for i in *.a; do echo $(AR) d $$i `$(AR) t $$i| egrep nunit`; done)

##############################################
#  beyond this points only mangement target. #
##############################################
clean :
	@echo Begin to clean
	$(RM) $(ALL_EXE)
	find . -type f -and -name "core" -or -name "*.[oa]" -or -name "*.so" -or -name "*.rpo" | xargs rm -f 
	@echo Clean is completed

prepare_build: mirror_build_dir build_include_dir dep

distclean: clean_config_debug_dir clean_include_dir clean_pkg
	cd ../build_linux && make -f ../src/Makefile.static clean clean_build_dir
	cd ../build_win32 && make -f ../src/Makefile.static clean clean_build_dir
	cd ../build_macos && make -f ../src/Makefile.static clean clean_build_dir

###############################################
#     experimentation to backup via duplicity #
###############################################

BACKUP_FNAME	= $(shell echo neoip.sos.`date +%Y%m%d`.tgz)
backup:
	cd ../../ && tar cvzf $(BACKUP_FNAME) yavipin
	cat ../../$(BACKUP_FNAME) | mcrypt > /tmp/$(BACKUP_FNAME).nc
	scp /tmp/$(BACKUP_FNAME).nc dedixl:workspace

