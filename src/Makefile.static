ALL_EXE += neoip-webpack
#ALL_EXE += neoip-btcli

all: $(ALL_EXE)

######################################################
# setup the command to build libraries shared/static on all NEOIP_TARGET
# - http://www.gnu.org/software/make/manual/make.html#Sequences
########
ifeq ($(NEOIP_TARGET), linux)
define build_sharedlib
	$(CXX) -shared -o $@ -Wl,-soname,$@ $^
endef
define build_staticlib
	$(AR) rcs $@ $^
	$(RANLIB) $@
endef
else ifeq ($(NEOIP_TARGET), win32)
define build_sharedlib
	$(CXX) -shared -o $@ -Wl,-soname,$@ $^
endef
define build_staticlib
	$(AR) rcs $@ $^
	$(RANLIB) $@
endef
else ifeq ($(NEOIP_TARGET), macos)
define build_sharedlib
	libtool -dynamic -flat_namespace -undefined dynamic_lookup -o $@ $^
endef
define build_staticlib
	$(AR) rcs $@ $^
	$(RANLIB) $@
endef
endif
######################################################

